{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/jobs-new/(.*)",
      "dest": "/jobs-new/$1"
    },
    {
      "src": "/abilities-gamerescape/(.*)",
      "dest": "/abilities-gamerescape/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
