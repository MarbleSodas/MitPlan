{
  "buildCommand": "bun run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/jobs-new/(.*)",
      "dest": "/jobs-new/$1"
    },
    {
      "src": "/abilities-gamerescape/(.*)",
      "dest": "/abilities-gamerescape/$1"
    },
    {
      "src": "/abilities-official/(.*)",
      "dest": "/abilities-official/$1"
    },
    {
      "src": "/icons/(.*)",
      "dest": "/icons/$1"
    },
    {
      "src": "/xivmitplan.ico",
      "dest": "/xivmitplan.ico"
    },
    {
      "src": "/favicon.ico",
      "dest": "/xivmitplan.ico"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
