{
  "rules": {
    "userProfiles": {
      ".read": true,
      ".write": true,
      "$userId": {
        ".read": true,
        ".write": true
      }
    },
    "plans": {
      ".read": true,
      ".write": true,
      ".indexOn": ["userId", "ownerId"],
      "$planId": {
        ".read": true,
        ".write": true
      }
    },
    "timelines": {
      ".read": true,
      ".write": true,
      ".indexOn": ["userId", "ownerId", "official"],
      "$timelineId": {
        ".read": true,
        ".write": "data.child('official').val() !== true || !data.exists()"
      }
    }
  }
}
